		 File file = new File("./dataSet.txt");		 
	        Reader reader;
			try {
				reader = new FileReader(file);
				  BufferedReader br = new BufferedReader(reader);			        
			      String line = "";
			      int total = 0;
			      String[] tabThreeelve = new String[2];
			      int cpt = 0;
			      int totalGrp = 0;
			        while((line = br.readLine()) != null) {		
			        	
			        	if(cpt==3) {
			        		
			        		int letter = 0;
			        		char[] tabC = line.toCharArray();
			        		
			        			for (int i =0; tabC.length > i; i++) {				        		
					        		for(int y = tabThreeelve[0].toCharArray().length;tabThreeelve[0].toCharArray().length>y;y++) {		
					        			
//					        			if(tabC[i]==tabC[y]) {
//					        				if(Character.isUpperCase(tabC[i])) {
//					        					letter += positions(tabC[i]) + 26;
//					        					System.out.println(tabC[i]);
//					        					y = tabC.length;
